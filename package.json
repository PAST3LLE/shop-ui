  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "analyze2": "webpack-bundle-analyzer bundle/output/path/stats.json",
    "lint": "npx prettier --write src --ignore-path ./.prettierignore",
    "start:mock": "REACT_APP_IS_MOCK=true && yarn start",
    "start": "PORT=8080 GENERATE_SOURCEMAP=false craco start",
    "build": "craco build",
    "build:analyze": "yarn build && yarn analyze",
    "build:serve": "DISABLE_ESLINT_PLUGIN='true' craco build && npx serve build -l 666",
    "test": "craco test --env=jsdom",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache",
    "postinstall": "yarn contracts:compile && yarn graphql:generate && yarn i18n:compile",
    "eject": "react-scripts eject",
    "dedup": "npx yarn-deduplicate",
    "graphql:generate": "DOTENV_CONFIG_PATH=\".env.local\" graphql-codegen --require dotenv/config --config codegen.yml ./src/shopify/graphql && yarn prettier --write src/shopify/graphql/types/_generated_.ts"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
