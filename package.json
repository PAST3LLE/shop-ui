  "scripts": {
    "analyze": "VITE_ANALYZE_BUNDLE=true yarn build",
    "lint": "npx prettier --write src --ignore-path ./.prettierignore",
    "start:mock": "VITE_IS_MOCK=true && yarn start",
    "start": "PORT=8080 GENERATE_SOURCEMAP=false vite dev",
    "serve": "npx serve dist -l 666",
    "build": "vite build",
    "build:analyze": "yarn build && yarn analyze",
    "build:serve": "DISABLE_ESLINT_PLUGIN='true' vite build && yarn serve",
    "test": "vite test --env=jsdom",
    "test:debug": "vite --inspect-brk test --runInBand --no-cache",
    "dedup": "npx yarn-deduplicate",
    "graphql:generate": "DOTENV_CONFIG_PATH=\".env.local\" graphql-codegen --require dotenv/config --config codegen.yml ./src/shopify/graphql && yarn prettier --write src/shopify/graphql/types/_generated_.ts"
